            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="language"/>
                <xs:element name="sampled" type="richtext"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

